<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="zikula_formicula_module.container.link_container.class">Zikula\FormiculaModule\Container\LinkContainer</parameter>
        <parameter key="twig.extension.zikula_formicula_module.class">Zikula\FormiculaModule\Twig\TwigExtension</parameter>
        <parameter key="zikula_formicula_module.listener.third_party_listener.class">Zikula\FormiculaModule\Listener\ThirdPartyListener</parameter>
        <parameter key="zikula_formicula_module.helper.environment_helper.class">Zikula\FormiculaModule\Helper\EnvironmentHelper</parameter>
        <parameter key="zikula_formicula_module.helper.captcha_helper.class">Zikula\FormiculaModule\Helper\CaptchaHelper</parameter>
    </parameters>
    <services>
        <service id="zikula_formicula_module.container.link_container" class="%zikula_formicula_module.container.link_container.class%">
            <argument type="service" id="translator.default" />
            <argument type="service" id="jms_i18n_routing.router" />
            <argument type="service" id="zikula_permissions_module.api.permission" />
            <tag name="zikula.link_container"/>
        </service>
        <service id="twig.extension.zikula_formicula_module" class="%twig.extension.zikula_formicula_module.class%" public="false">
            <argument type="service" id="zikula_formicula_module.helper.captcha_helper" />
            <tag name="twig.extension" />
        </service>
        <service id="zikula_formicula_module.listener.third_party_listener" class="%zikula_formicula_module.listener.third_party_listener.class%">
            <tag name="kernel.event_subscriber" />
        </service>
        <service id="zikula_formicula_module.helper.captcha_helper" class="%zikula_formicula_module.helper.captcha_helper.class%" lazy="true">
            <argument type="service" id="translator.default" />
            <argument type="service" id="zikula_extensions_module.api.variable" />
            <argument type="service" id="zikula_permissions_module.api.permission" />
            <argument type="service" id="zikula_formicula_module.helper.environment_helper" />
            <argument type="service" id="session" />
        </service>
        <service id="zikula_formicula_module.helper.environment_helper" class="%zikula_formicula_module.helper.environment_helper.class%" lazy="true">
            <argument type="service" id="kernel" />
            <argument type="service" id="translator.default" />
            <argument type="service" id="zikula_extensions_module.api.variable" />
            <argument type="service" id="session" />
        </service>
    </services>
</container>
