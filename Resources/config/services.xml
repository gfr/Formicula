<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="zikula_formicula_module.container.link_container.class">Zikula\FormiculaModule\Container\LinkContainer</parameter>
        <parameter key="zikula_formicula_module.helper.environment_checker.class">Zikula\FormiculaModule\Helper\EnvironmentChecker</parameter>
    </parameters>
    <services>
        <service id="zikula_formicula_module.container.link_container" class="%zikula_formicula_module.container.link_container.class%">
            <argument type="service" id="translator.default" />
            <argument type="service" id="jms_i18n_routing.router" />
            <argument type="service" id="zikula_permissions_module.api.permission" />
            <tag name="zikula.link_container"/>
        </service>
        <service id="zikula_formicula_module.helper.environment_checker" class="%zikula_formicula_module.helper.environment_checker.class%" lazy="true">
            <argument type="service" id="kernel" />
            <argument type="service" id="translator.default" />
            <argument type="service" id="zikula_extensions_module.api.variable" />
            <argument type="service" id="session" />
        </service>
    </services>
</container>
